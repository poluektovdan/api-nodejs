{
	"name": "api-nodejs",
	"version": "1.0.0",
	"description": "API для учебного проекта",
	"main": "index.js",
	"type": "commonjs",
	"scripts": {
		"start": "node ./dist/main.js",
		"dev": "nodemon",
		"dev:inspect": "nodemon -e ts,json --exec node --inspect=localhost:9222 -r ts-node/register src/main.ts",
		"lint": "eslint ./src/**",
		"generate": "prisma generate",
		"lint:fix": "eslint ./src/** --fix",
		"build": "tsc",
		"test": "jest",
		"test:e2e": "jest --config jest.e2e.config.ts --coverage"
	},
	"author": "Daniil Poluektov",
	"license": "ISC",
	"dependencies": {
		"@prisma/client": "^5.14.0",
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.20.2",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.1",
		"dotenv": "^16.4.5",
		"express": "^4.19.2",
		"inversify": "^6.0.2",
		"jsonwebtoken": "^9.0.2",
		"reflect-metadata": "^0.2.2",
		"tslog": "^4.9.2"
	},
	"devDependencies": {
		"@types/bcryptjs": "^2.4.6",
		"@types/express": "^4.17.21",
		"@types/jest": "^29.5.12",
		"@types/jsonwebtoken": "^9.0.6",
		"@types/supertest": "^6.0.2",
		"@typescript-eslint/eslint-plugin": "^7.8.0",
		"@typescript-eslint/parser": "^7.8.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"jest": "^29.7.0",
		"nodemon": "^3.1.0",
		"prettier": "^3.2.5",
		"prisma": "^5.14.0",
		"supertest": "^7.0.0",
		"ts-jest": "^29.1.2",
		"ts-node": "^10.9.2",
		"typescript": "^5.4.5"
	}
}
